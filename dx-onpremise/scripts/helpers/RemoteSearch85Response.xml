    <data key='user.cw.userid,com.ibm.websphere.PORTAL.REMOTESEARCH.v85' value='wpsadmin'/>
    <data key='user.cw.password,com.ibm.websphere.PORTAL.REMOTESEARCH.v85' value='4taxKp1Gj5q5aCi+LjSKHQ=='/>
    <data key='user.iim.currentlocale,com.ibm.websphere.PORTAL.REMOTESEARCH.v85' value='en'/>
    <data key='user.configengine.binaryLocation,com.ibm.websphere.PORTAL.REMOTESEARCH.v85' value='/opt/HCL/WebSphere/PortalRemoteSearch/ConfigEngine'/>
  </profile>
  <install>
    <!-- Portal Remote Search 8.5.0.0 CFXX -->
    <offering profile='Portal Remote Search' id='com.ibm.websphere.PORTAL.REMOTESEARCH.v85' features='main.ce,main.dcs.install,main.rs.deploy,main.rs.install'/>
