  <profile id='Portal Remote Search' installLocation='${installLocation.Portal_Remote_Search}'>
    <data key='cic.selector.arch' value='x86_64'/>
    <!-- Data for Portal Remote Search: -->
    <data key='user.was.installLocation,com.ibm.websphere.PORTAL.REMOTESEARCH.v85' value='/opt/HCL/AppServer'/>
